{% extends 'base.html.twig' %}

{% block title %}OperatingHistory{% endblock %}

{% block body %}
    <h1>OperatingHistory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ operating_history.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ operating_history.date ? operating_history.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ operating_history.duration }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ operating_history.status }}</td>
            </tr>
            <tr>
                <th>ConsumedData</th>
                <td>{{ operating_history.consumedData }}</td>
            </tr>
            <tr>
                <th>DataSent</th>
                <td>{{ operating_history.dataSent }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_operating_history_index') }}">back to list</a>

    <a href="{{ path('app_operating_history_edit', {'id': operating_history.id}) }}">edit</a>

    {{ include('operating_history/_delete_form.html.twig') }}
{% endblock %}
